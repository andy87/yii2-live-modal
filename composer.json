{
  "name": "andy87/yii2-live-modal",
  "description": "PHP library for Yii2 - developed by and_y87",
  "keywords": ["and_y87", "php", "yii2", "live", "modal"],
  "homepage": "https://andy87.ru",
  "license": "CC-BY-SA-4.0",

  "version": "1.0.0",
  "minimum-stability": "dev",

  "prefer-stable": true,

  "authors": [
    {
      "name": "Kidin Andrey",
      "email": "kidin_a@list.ru"
    }
  ],
  "require": {
    "php": ">=8.0"
  },

  "require-dev": {
    "yiisoft/yii2": "~2.0.45"
  },

  "autoload": {
    "psr-4": {
      "andy87\\yii2\\live_modal\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "andy87\\yii2\\live_modal\\": "src/"
    }
  },

  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },

  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
